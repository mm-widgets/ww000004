{
	"name": "@mmstudio/base",
	"version": "1.0.0",
	"description": "$desc",
	"scripts": {
		"test": "npm run build && karma start --single-run",
		"watch": "tsc -w & karma start",
		"clean": "del-cli ./dist/",
		"lint": "eslint ./src/**/*.ts",
		"compile-amd": "tsc -m amd -d false --outFile ./dist/main.js",
		"compile-cjs": "tsc",
		"compile": "npm run compile-cjs && npm run compile-amd",
		"build": "npm run clean && npm run lint && npm run compile",
		"up": "git merge master"
	},
	"repository": {
		"type": "git",
		"url": "https://gitee.com/mmstudio/base"
	},
	"main": "dist/index.js",
	"module": "dist/index.js",
	"types": "dist/index.d.ts",
	"keywords": [
		"mm",
		"mmstudio",
		"蛮蛮",
		"蛮蛮工作室",
		"插件式",
		"原子操作",
		"控件",
		"框架"
	],
	"author": {
		"name": "$user",
		"email": "$email@xxx.com"
	},
	"maintainers": [
		{
			"name": "taoqf",
			"email": "tao_qiufeng@126.com"
		}
	],
	"license": "MIT",
	"dependencies": {},
	"devDependencies": {
		"@typescript-eslint/eslint-plugin": "latest",
		"@typescript-eslint/parser": "latest",
		"chai": "latest",
		"del-cli": "latest",
		"eslint": "latest",
		"eslint-config-prettier": "latest",
		"eslint-plugin-import": "latest",
		"karma": "latest",
		"karma-chai": "latest",
		"karma-chrome-launcher": "latest",
		"karma-mocha": "latest",
		"karma-webpack": "latest",
		"mocha": "latest",
		"typescript": "next",
		"webpack": "latest",
		"webpack-cli": "latest"
	}
}